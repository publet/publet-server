{
    "name": "Publet",
    "version": "0.2.3",
    "manifest_version": 2,
    "icons": {
        "128": "images/button.png"
    },
    "description": "Publet",
    "homepage_url": "https://publet.com",
    "permissions": [
        "tabs",
        "storage",

        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "images/button.png",
        "default_title": "Publet",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "scripts/jquery-1.7.2.min.js",
            "scripts/publet.main.js"
        ]
    },
    "content_scripts": [{
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }]
}
